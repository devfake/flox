{
  "name": "flox-file-parser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:vagrant": "(npm i --no-bin-links) > /dev/null 2>&1 || npm i --no-bin-links",
    "start": "node server.js",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --timeout 15000 --compilers js:babel-core/register './app/**/*.spec.js'",
    "coverage": "NODE_ENV=test node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha 'app/**/*.js' -- --require babel-register",
    "test_ci": "for i in $(seq 1 100); do echo Run $i; npm test; if [ $? -ne 0 ]; then break; fi; done",
    "mocha": "./node_modules/mocha/bin/mocha",
    "test-watch": "NODE_ENV=test ./node_modules/mocha/bin/mocha --reporter min --watch --compilers js:babel-core/register './app/**/*.spec.js'",
    "eslint": "node ./node_modules/eslint/bin/eslint.js './app/**/*.js'",
    "sequelize": "cd database && ../node_modules/sequelize-cli/bin/sequelize",
    "db:create": "node ./database/scripts/db.js create",
    "db:drop": "node ./database/scripts/db.js drop",
    "db:migrate": "cd database && npm run sequelize db:migrate",
    "db:setup": "npm run db:drop && npm run db:create && npm run db:migrate"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "3.12.2",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^3.2.0",
    "sinon": "^1.17.6",
    "supertest": "^2.0.1"
  },
  "dependencies": {
    "express": "^4.14.0",
    "mysql": "2.12.0",
    "sequelize-cli": "^2.5.1",
    "sequelize": "^3.28.0",
    "video-name-parser": "^1.4.0"
  }
}
